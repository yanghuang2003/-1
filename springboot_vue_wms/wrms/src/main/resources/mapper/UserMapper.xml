<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ruoyi.wrms.mapper.UserMapper">
    <select id="listAll" resultType="com.ruoyi.wrms.pojo.User">
        select * from user
    </select>
    <select id="findByNo" resultType="com.ruoyi.wrms.pojo.User">
        select * from user where no=#{no}
    </select>
</mapper>
